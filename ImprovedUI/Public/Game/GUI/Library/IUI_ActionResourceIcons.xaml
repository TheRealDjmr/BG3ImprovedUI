	<!-- Setup default style that is inherited before changes are applied, do not touch-->
	<ImageSource x:Key="TypeIdToSourceFallback">pack://application:,,,/GustavNoesisGUI;component/Assets/Shared/ico_red_star.png</ImageSource>

	<Style TargetType="Image" x:Key="IUI_ActionResourceTypeIdToSourceDefault">
		<Style.Triggers>
			<!-- Dynamically try and find an image by the Type ID in the regular Larian folder -->
			<DataTrigger Binding="{Binding TypeId, Converter={StaticResource NullToBoolTrueConverter}, ConverterParameter='EmptyString'}" Value="False">
				<Setter Property="Source">
					<Setter.Value>
						<MultiBinding Converter="{StaticResource IconIdToSourceConverter}" FallbackValue="{StaticResource TypeIdToSourceFallback}">
							<Binding Source="Assets/Shared/Resources/"/>
							<Binding Path="TypeId"/>
						</MultiBinding>
					</Setter.Value>
				</Setter>
			</DataTrigger>
			<!-- Proof working with action point icon -->
			<!--<DataTrigger Binding="{Binding TypeId}" Value="ActionPoint">
				<Setter Property="Source" Value="{StaticResource IconActionPoint}"/>
			</DataTrigger>-->
			<DataTrigger Binding="{Binding TypeId}" Value="BonusActionPoint">
				<Setter Property="Source" Value="{StaticResource IconBonusActionPoint}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding TypeId}" Value="ReactionActionPoint">
				<Setter Property="Source" Value="{StaticResource IconReactionPoint}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding TypeId}" Value="Movement">
				<Setter Property="Source" Value="{StaticResource IconMovement}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding TypeId}" Value="WarlockSpellSlot">
				<Setter Property="Source" Value="{StaticResource IconWarlockSpellSlot}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding TypeId}" Value="SpellSlot">
				<Setter Property="Source" Value="{StaticResource IconSpellSlot}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding TypeId}" Value="SorceryPoint">
				<Setter Property="Source" Value="{StaticResource SorceryPoint}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding TypeId}" Value="KiPoint">
				<Setter Property="Source" Value="{StaticResource KiPoint}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding TypeId}" Value="LayOnHandsCharge">
				<Setter Property="Source" Value="{StaticResource LayOnHandsCharge}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding TypeId}" Value="Rage">
