				<Setter Property="Source">
					<Setter.Value>
						<MultiBinding Converter="{StaticResource IconIdToSourceConverter}" FallbackValue="{StaticResource IUI_GenericClassMainIcon}">
							<Binding Source="Assets/ClassIcons/"/>
							<Binding Path="IDString"/>
